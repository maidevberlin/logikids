{
  "name": "@logikids/backend",
  "version": "0.1.0",
  "type": "module",
  "main": "src/index.ts",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./src/index.ts"
    },
    "./tasks/types": {
      "types": "./dist/src/tasks/types.d.ts",
      "default": "./src/tasks/types.ts"
    },
    "./tasks/subjects": {
      "types": "./dist/src/tasks/subjects/index.d.ts",
      "default": "./src/tasks/subjects/index.ts"
    }
  },
  "scripts": {
    "postinstall": "tsc --project tsconfig.build.json || true",
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "build:types": "tsc --project tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "migrate": "bun database/migrate.ts",
    "generate:prompt": "bun src/scripts/generate-prompt.ts",
    "generate:task": "bun src/scripts/generate-task.ts",
    "check:prompts": "bun src/scripts/check-prompts.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@sentry/bun": "^10.29.0",
    "@trpc/client": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "@types/js-yaml": "^4.0.9",
    "chokidar": "^4.0.3",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8",
    "js-yaml": "4.1.1",
    "jsonwebtoken": "^9.0.2",
    "latex-to-speech": "^0.0.4",
    "node-tikzjax": "^1.0.3",
    "openai": "^4.76.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "strip-markdown": "^6.0.0",
    "tsyringe": "^4.10.0",
    "uuid": "^13.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.7",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^11.0.0",
    "bun-types": "^1.1.38",
    "nodemon": "^3.1.11",
    "supertest": "^7.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
